<container>
  <vbox>
    <hbox class="{{styles.link}}">
      {{#each model as |user|}}
        <centered>
          {{#link-to 'users.show' user.id}}
            {{user.firstName}}
          {{/link-to}}
        </centered>
      {{/each}}
    </hbox>
    <hbox>{{outlet}}</hbox>
  </vbox>
</container>
